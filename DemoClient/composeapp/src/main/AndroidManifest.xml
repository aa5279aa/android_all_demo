<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xt.composeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DemoClient">
        <activity
            android:name=".ComposeActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DemoClient">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".compose.activity.ComposeDataBindingActivity"/>
        <activity android:name=".compose.activity.ComposeMVIActivity"/>
        <activity android:name=".compose.activity.ComposeListActivity"/>
    </application>

</manifest>